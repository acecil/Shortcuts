/* Configure the current version of the Shortcuts application. */

#define SHORTCUTS_MAJOR_VERSION 0
#define SHORTCUTS_MINOR_VERSION 2
#define SHORTCUTS_MICRO_VERSION 1

#define xstr(s) str(s)
#define str(s) #s

#define SHORTCUTS_VERSION_STRING xstr(SHORTCUTS_MAJOR_VERSION) "." xstr(SHORTCUTS_MINOR_VERSION) "." xstr(SHORTCUTS_MICRO_VERSION)

